{
    "name": "Grader Helper",
    "description": "This Extension helps teaching assistants to grade student assignments",
    "version": "1.0",
    "manifest_version": 3,
    "permissions":[
      "contextMenus"
    ],
    "action": {
      "default_popup": "popup/index.html",
      "default_icon": "images/logo.png"
    },
    "icons": {
      "16": "images/logo.png",
      "32": "images/logo.png",
      "48": "images/logo.png",
      "128": "images/logo.png"
    },
    "background": {
      "service_worker": "service_worker.js",
      "type":"module"
    },
    "content_scripts": [
      {
        "js": ["scripts/content.js"],
        "matches": [
          "https://learn.dcollege.net/ultra/*"
        ]
      }
    ]
  }